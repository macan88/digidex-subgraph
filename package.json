{
  "name": "digidex-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/macan88/digidex-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create digi-dex/digidex-subgraph --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy digi-dex/digidex-subgraph --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "create-remote": "graph create digi-dex/digidex-subgraph --node http://188.34.132.202:8020",
    "deploy": "graph deploy digi-dex/digidex-subgraph --ipfs http://161.97.117.103:5001/ --node http://188.34.132.202:8020/ --debug",
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME /Digichain --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "watch-local": "graph deploy graphprotocol/DigiDex2 --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
